{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": true,
    "indent": [true, 2],
    "interface-name": true,
    "jsdoc-format": true,
    "member-ordering": [
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-consecutive-blank-lines": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-keys": true,
    "no-duplicate-variable": true,
    "no-shadowed-variable": true,
    "no-empty": true,
    "no-eval": true,
    "no-require-imports": true,
    "no-trailing-comma": true,
    "no-trailing-whitespace": true,
    "no-unreachable": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "one-line": [
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [true, "double"],
    "radix": true,
    "semicolon": true,
    "switch-default": true,
    "triple-equals": true,
    "typedef": [
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ],
    "use-strict": "check-module",
    "whitespace": [
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
